<td class="test-name">
    {{ name }}
    {{#if failedOnLastBuild}}
        [<a href="{{ mostRecentNode }}">{{ mostRecentNode }}</a>]
    {{/if}}
</td>
<td class="test-recent">
    {{ mostRecent }}
    {{#if failedOnLastBuild}}
        <span class="label label-important">Last Build</span>
    {{/if}}
</td>
<td class="test-count">{{ count }}</td>
<td class="test-rate">{{ rate }}%</td>
<td class="test-score">{{ score }}</td>
